# WhackRock Treasury Deployment Configuration for Base Mainnet

# Private key for deployment (without 0x prefix)
PRIVATE_KEY=your_private_key_here_without_0x_prefix

# Base mainnet addresses
USDC_ADDRESS=0xd9aAEc86B65D86f6A7B5B1b0c42FFA531710b6CA   # USDbC on Base
WETH_ADDRESS=0x4200000000000000000000000000000000000006   # WETH on Base
UNISWAP_V3_FACTORY=0x33128a8fC17869897dcE68Ed026d694621f6FDfD   # Uniswap V3 Factory on Base
WETH_USDC_POOL=0xB4885Bc63399BF5518b994c1d0C153334Ee579D0   # WETH/USDbC pool on Base
UNIVERSAL_ROUTER=0x198EF79F2a137D42A775593a8017DA68eCb2D8DD   # Uniswap Universal Router on Base
WRK_REWARDS_ADDRESS=0x123...   # Replace with your rewards address

# Initial allowed assets (comma-separated list of addresses)
# Format: ASSET1,ASSET2,ASSET3,...
# Must include USDC.b address
INITIAL_ASSETS=0xd9aAEc86B65D86f6A7B5B1b0c42FFA531710b6CA,0x4200000000000000000000000000000000000006,0x2Ae3F1Ec7F1F5012CFEab0185bfc7aa3cf0DEc22,0xEB466342C4d449BC9f53A865D5Cb90586f405215

# RPC URL for Base mainnet
RPC_URL=https://mainnet.base.org

# Etherscan API key for verification (optional)
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# Gas settings for Base (adjust as needed)
GAS_PRICE=1000000000   # 1 gwei
PRIORITY_GAS_PRICE=2000000000  # 2 gwei

# Deployment settings
# Set to true to skip deployment of initial vault
SKIP_INITIAL_VAULT=false

# Initial vault parameters
INITIAL_VAULT_NAME="WhackRock Treasury Alpha"
INITIAL_VAULT_SYMBOL="WRTA"
INITIAL_VAULT_FEE_BPS=200  # 2% management fee
INITIAL_VAULT_TAG="ALPHA"

# Notes on additional assets:
# 0x2Ae3F1Ec7F1F5012CFEab0185bfc7aa3cf0DEc22 = USDT on Base
# 0xEB466342C4d449BC9f53A865D5Cb90586f405215 = cbETH on Base